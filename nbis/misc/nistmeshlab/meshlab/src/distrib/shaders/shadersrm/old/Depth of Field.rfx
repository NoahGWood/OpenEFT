    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Base;
uniform float Ks;
uniform float Kd;


varying vec3 vNormal;
varying vec3 vViewVec;

void main(void)
{
   vec3 base = texture2D(Base, gl_TexCoord[0].st).rgb;
   
   vec3 nNormal = normalize(vNormal);
   vec3 nLight = normalize(gl_LightSource[0].position.xyz);

   // Basic lighting
   float diffuse = clamp(dot(nLight, nNormal), 0.0, 1.0);
   float specular = pow(clamp(dot(reflect(-normalize(vViewVec), nNormal), nLight),0.0, 1.0), 16.0);
   vec3  light = Kd * diffuse * base + Ks * specular;

   // We'll use the distance to decide how much blur we want
   float dist = length(vViewVec);

   gl_FragColor = vec4( light, dist);
}]]></RmShaderCode></RmGLShader>
    <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float distanceScale;

varying vec3 vNormal;
varying vec3 vViewVec;

void main(void)
{
   gl_Position = ftransform();
   gl_TexCoord[0] = gl_MultiTexCoord0;
   
   // Eye-space lighting
   vNormal = gl_NormalMatrix * gl_Normal;
   
   // We multiply with distance scale in the vertex shader
   // instead of the fragment shader to improve performance.
   vViewVec = -vec3(distanceScale * gl_ModelViewMatrix * gl_Vertex);

}]]></RmShaderCode></RmGLShader>
