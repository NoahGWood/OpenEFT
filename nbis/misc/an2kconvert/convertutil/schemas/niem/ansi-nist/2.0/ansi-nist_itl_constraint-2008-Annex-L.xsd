      <xsd:documentation>Rogation about the horizontal &apos;x&apos; axis.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PosePitchUncertaintyValue" type="niem-xsd:nonNegativeInteger">
    <xsd:annotation>
      <xsd:documentation>The pitch uncertainty.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PoseRollAngleMeasure" type="niem-xsd:integer">
    <xsd:annotation>
      <xsd:documentation>Rotation about the &apos;z&apos; axis.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PoseRollUncertaintyValue" type="niem-xsd:nonNegativeInteger">
    <xsd:annotation>
      <xsd:documentation>The roll uncertainty.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PoseYawAngleMeasure" type="niem-xsd:integer">
