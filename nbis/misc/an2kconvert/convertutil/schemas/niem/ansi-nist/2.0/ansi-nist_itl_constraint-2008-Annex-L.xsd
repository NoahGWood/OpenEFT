    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PhysicalFeatureWidthMeasure" type="niem-xsd:nonNegativeInteger">
    <xsd:annotation>
      <xsd:documentation>The feature&apos;s width in whole centimeters</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PosePitchAngleMeasure" type="niem-xsd:integer">
