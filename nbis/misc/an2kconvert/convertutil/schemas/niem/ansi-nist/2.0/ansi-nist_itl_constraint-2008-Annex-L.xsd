    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="ReadingSystemName" type="nc:TextType">
    <xsd:annotation>
      <xsd:documentation>The name of the system.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="ReadingSystemSubsystemIdentification" type="nc:IdentificationType">
