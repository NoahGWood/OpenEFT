        <i:Base i:name="RecordImage"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PhysicalFeatureNCICCode" type="fbi:SMTCodeType">
    <xsd:annotation>
      <xsd:documentation>NCIC code for a particular scar, mark, or tattoo.</xsd:documentation>
