    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="QualityMeasureVendorIdentification" type="nc:IdentificationType">
    <xsd:annotation>
      <xsd:documentation>From a registry table, the ID of the vendor of the quality algorithm.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="QualityValue" type="niem-xsd:integer">
