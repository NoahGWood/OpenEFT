    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PhysicalFeatureSecondaryColorCode" type="ansi-nist:COLCodeType">
    <xsd:annotation>
      <xsd:documentation>Secondary Color of  a tattoo in an image.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PhysicalFeatureSize" type="ansi-nist:PhysicalFeatureSizeType">
