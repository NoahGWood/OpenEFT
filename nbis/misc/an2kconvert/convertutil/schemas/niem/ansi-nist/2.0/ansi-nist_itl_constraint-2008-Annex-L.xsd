      <xsd:documentation>Predicted matching performance.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="ReadingSystemCodingMethodCode" type="ansi-nist:OFRCodeType">
