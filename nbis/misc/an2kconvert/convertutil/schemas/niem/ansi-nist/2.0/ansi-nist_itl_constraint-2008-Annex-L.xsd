    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PoseYawUncertaintyValue" type="niem-xsd:nonNegativeInteger">
    <xsd:annotation>
      <xsd:documentation>The yaw uncertainty.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PositionBottomCoordinateValue" type="niem-xsd:nonNegativeInteger">
