  <!-- ======================================================= -->
  <xsd:element name="PositionThetaAngleMeasure" type="niem-xsd:nonNegativeInteger">
    <xsd:annotation>
      <xsd:documentation>The minutia&apos;s theta angle</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ======================================================= -->
  <xsd:element name="PositionTopCoordinateValue" type="niem-xsd:nonNegativeInteger">
